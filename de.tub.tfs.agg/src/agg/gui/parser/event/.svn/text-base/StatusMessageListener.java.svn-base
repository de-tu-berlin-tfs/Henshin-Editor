package agg.gui.parser.event;

import java.util.EventListener;

/**
 * Listens to all messages of the status bar.
 * 
 * @version $Id: StatusMessageListener.java,v 1.2 2007/09/10 13:05:50 olga Exp $
 * @author $Author: olga $
 */
public interface StatusMessageListener extends EventListener {

	/**
	 * Receives a message for the status bar
	 * 
	 * @param sme
	 *            The new message
	 */
	public void newMessage(StatusMessageEvent sme);

}
